<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="firstPage" parent="AppTheme">
        <item name="colorPrimary">@color/purple</item>
        <item name="colorPrimaryDark">@color/purpleDark</item>
        <item name="android:title">@string/appName</item>

    </style>
    <style name="tab1" parent="AppTheme">
        <item name="colorPrimary">@color/tab1</item>
        <item name="colorPrimaryDark">@color/tab1dark</item>
        <item name="android:title">@string/tarakoneshDaramadi</item>


    </style>
    <style name="tab2" parent="AppTheme">
        <item name="colorPrimary">@color/tab2</item>
        <item name="colorPrimaryDark">@color/tab2dark</item>
        <item name="android:title">@string/tarakoneshHazine</item>
    </style>
    <style name="tab3" parent="AppTheme">
        <item name="colorPrimary">@color/tab3</item>
        <item name="colorPrimaryDark">@color/tab3</item>
    </style>



</resources>
